{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"TwitchChat Plugin Poll Sample",
    "version":"1.0.0.5",
    "minimum-overwolf-version": "0.77.4.0",
    "author":"Overwolf",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":"Sample app for creating a poll using the Twitchchat plugin"
  },
  "data": {
    "start_window":"index",
    "windows":{
      "index":{
        "file":"index.html",
        "transparent":false,
        "resizable":true,
        "show_in_taskbar": true,
        "size": {
          "width": 600,
          "height": 560
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        },
        "min_size": {
          "width": 600,
          "height": 560
        }
      }
    },
    "plugins": [
      "npTwitchChatPlugin.dll"
    ],
    "externally_connectable":{
      "matches":[
        "https://*.twitch.tv",
        "http://*.twitch.tv",
        "http://*.overwolf.com"
      ]
    }
  }
}